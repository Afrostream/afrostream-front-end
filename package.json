{
  "name": "afrostream-front-end",
  "version": "1.1.3",
  "description": "Universal Afrostream with React + Redux.",
  "main": "server/index.js",
  "scripts": {
    "app": "./node_modules/.bin/babel-node server/app",
    "lint": "node ./node_modules/eslint/bin/eslint.js -c .eslintrc server",
    "clean": "rm -rf dist && mkdir dist",
    "build:prod": "./node_modules/.bin/babel-node ./node_modules/webpack/bin/webpack.js --progress --verbose --display-error-details --config webpack/prod.config.js",
    "build": "npm run clean && npm run build:prod",
    "watch": "./node_modules/.bin/babel-node webpack/webpack-dev-server.js --hot --inline --watch",
    "dev": "echo 'requiring node v6.9.1' && . ~/.nvm/nvm.sh && nvm use 6.9.1 && node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch\" \"npm run app\"",
    "staging": "export LOAD_STAGING=true && export API_END_POINT=https://afr-api-v1-staging.herokuapp.com && npm run dev",
    "start": "node ./dist/server.js",
    "debug": "node --debug ./dist/server.js",
    "postinstall": "npm run-script build",
    "shrink": "shrinkwarp --ignore fsevents",
    "postshrinkwrap": "npm run shrink"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Afrostream/afrostream-front-end"
