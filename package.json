{
  "name": "afrostream-front-end",
  "version": "1.0.0",
  "description": "Universal Afrostream with React + Redux.",
  "main": "lib/server.js",
  "scripts": {
    "express": "./node_modules/.bin/babel-node lib/express",
    "deploy": "./node_modules/.bin/babel-node lib/deploy",
    "lint": "node ./node_modules/eslint/bin/eslint.js -c .eslintrc src",
    "build": "./node_modules/.bin/babel-node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack/prod.config.js",
    "watch-client-dev-server": "./node_modules/.bin/babel-node  webpack/webpack-dev-server.js",
    "watch-client-webpack": "./node_modules/.bin/babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack/dev.config.js --hot --progress --colors --inline  --port 8081 --host localhost --content-base dist/js",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client-dev-server\"  \"npm run express\"",
    "start": "npm run express",
    "test-prod": "NODE_ENV=production PORT=8080 npm run build && NODE_ENV=production PORT=8080 npm run express",
    "postinstall": "node lib/deploy"
  },
  "keywords": [
    "afrostream",
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Afrostream/afrostream-front-end"
  },
  "homepage": "https://github.com/Afrostream/afrostream-front-end",
  "author": "<pott.benjamin@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "del": "^1.2.0",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^2.5.2",
    "imagemin": "^3.2.0",
    "jshint": "^2.8.0",
    "node-libs-browser": "^0.5.2",
    "node-notifier": "^4.2.3",
    "react-hot-loader": "^1.2.8",
    "webpack-dev-server": "^1.10.0"
  },
  "dependencies": {
    "autoprefixer-core": "^5.2.1",
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.12",
    "babel-core": "^5.6.15",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.7.0",
    "bluebird": "^2.9.30",
    "bootstrap": "^3.3.5",
    "browser-sync-webpack-plugin": "^0.1.3",
    "classnames": "^2.1.3",
    "compression": "^1.5.0",
    "concurrently": "^0.1.1",
    "css-loader": "^0.15.1",
    "cssnext-loader": "1.0.1",
    "express": "^4.13.0",
    "express-handlebars": "^2.0.1",
    "express-session": "^1.11.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.3.0",
    "gsap": "^1.17.0",
    "handlebars": "^3.0.3",
    "html-loader": "^0.3.0",
    "image-webpack-loader": "^1.6.1",
    "immutable": "^3.7.4",
    "jshint-loader": "^0.8.3",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "lesshat": "^3.0.2",
    "lodash": "^3.9.3",
    "markdown-loader": "^0.1.3",
    "normalize.css": "^3.0.3",
    "parse-link-header": "^0.2.0",
    "penner": "^0.1.2",
    "postcss-loader": "^0.5.1",
    "qs": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react": "^0.13.3",
    "react-router": "^1.0.0-beta3",
    "react-redux": "^0.2.2",
    "redux-devtools": "0.1.2",
    "redux": "^1.0.0-rc",
    "serve-favicon": "^2.3.0",
    "stats-webpack-plugin": "^0.2.1",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.3",
    "superagent": "^1.2.0",
    "url-loader": "^0.5.6",
    "video.js": "^4.12.12",
    "videojs-contrib-hls": "git+https://github.com/Afrostream/videojs-contrib-hls.git#master",
    "videojs-swf": "^4.7.2",
    "webpack": "^1.10.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
