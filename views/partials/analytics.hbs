<!-- Google Analytics Content Experiment A/B Multiples testcode -->
<script type="text/javascript">
  var __ANALYTICS_STATE__ = {
    GA: {
      variations: []
    }
  }
</script>
<script src="//www.google-analytics.com/cx/api.js"></script>
{{#each GAabCodes}}
<!--<script src="//www.google-analytics.com/cx/api.js?experiment={{{this.key}}}"></script>-->
<script type="text/javascript">
  var choosedVariationAlalytics = cxApi.getChosenVariation('{{{this.key}}}')
  if (choosedVariationAlalytics === cxApi.NO_CHOSEN_VARIATION) {
    choosedVariationAlalytics = Math.floor(Math.random() * ({{{this.nbVariations}}} +1));
  }
  __ANALYTICS_STATE__.GA.variations.push({
    key: '{{{this.key}}}',
    name: '{{{this.name}}}',
    choose: choosedVariationAlalytics
  });
  cxApi.setChosenVariation(choosedVariationAlalytics, '{{{this.key}}}');
</script>
{{/each}}
<!-- END Google Analytics Content Experiment A/B testcode -->
