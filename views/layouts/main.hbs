<!DOCTYPE html>
<html data-cast-api-enabled="true">
<head>
    <title>Afrostream.tv</title>
    <meta content="minimum-scale=1.0, width=device-width, maximum-scale=1.0, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <!-- STREAMROOT STYLE -->
    <!--<link href="//files.streamroot.io/release/latest/wrappers/videojs/streamroot.css" rel="stylesheet">-->
    <meta charset="UTF-8"/>
  {{#each cssPaths}}
      <link rel="stylesheet" href="{{.}}">
  {{/each}}

    <!-- Streamroot player -->
    <!--<script src="//files.streamroot.io/release/latest/api.js"></script>-->
</head>
<body>
{{hash}}

<div id="main">{{{body}}}</div>
<script>
    __INITIAL_STATE__ = {{{json-stringify initialState}}};
</script>

{{#each jsPaths}}
    <script src="{{.}}" type="text/javascript"></script>
{{/each}}

<script>
    window.intercomSettings = {
        app_id: 'k3klwkxq'
    };
</script>
<script>
    (function () {
        var w = window;
        var ic = w.Intercom;
        if (typeof ic === 'function') {
            ic('reattach_activator');
            ic('update', intercomSettings);
        } else {
            var d = document;
            var i = function () {
                i.c(arguments)
            };
            i.q = [];
            i.c = function (args) {
                i.q.push(args)
            };
            w.Intercom = i;
            function l() {
                var s = d.createElement('script');
                s.type = 'text/javascript';
                s.async = true;
                s.src = 'https://widget.intercom.io/widget/k3klwkxq';
                var x = d.getElementsByTagName('script')[0];
                x.parentNode.insertBefore(s, x);
            }

            if (w.attachEvent) {
                w.attachEvent('onload', l);
            } else {
                w.addEventListener('load', l, false);
            }
        }
    })()
</script>

<!-- javascripts -->
<script src="//www.gstatic.com/cv/js/sender/v1/cast_sender.js" type="text/javascript"></script>
<script src="//js.recurly.com/v3/recurly.js" type="text/javascript"></script>
</body>
</html>
