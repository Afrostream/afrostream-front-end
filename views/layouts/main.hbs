<!DOCTYPE html>
<html data-cast-api-enabled="true">
<head>
    <meta charset="UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    {{{title}}}
    {{{meta}}}
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0">
    <meta name="robots" content="noindex, nofollow">
    <meta name="version" content="Afrostream ({{{version}}})">
    {{{link}}}
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <!-- STREAMROOT STYLE -->
    <!--<link href="//files.streamroot.io/release/latest/wrappers/videojs/streamroot.css" rel="stylesheet">-->
    {{#each cssPaths}}
      <link rel="stylesheet" href="{{.}}">
    {{/each}}

    <!-- Streamroot player -->
    <!--<script src="//files.streamroot.io/release/latest/api.js"></script>-->
</head>
<body>
{{hash}}

<div id="main">{{{body}}}</div>
<script>
    __INITIAL_STATE__ = {{{json-stringify initialState}}};
</script>

{{#each jsPaths}}
    <script src="{{.}}" type="text/javascript"></script>
{{/each}}

<!-- javascripts -->
<script src="//www.gstatic.com/cv/js/sender/v1/cast_sender.js" type="text/javascript"></script>
<script src="//js.recurly.com/v3/recurly.js" type="text/javascript"></script>
</body>
</html>
