<!DOCTYPE html>
<html data-cast-api-enabled="true">
<head>
  <script src="//cdn.optimizely.com/js/4240081832.js"></script>
  <meta charset="UTF-8"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  {{{title}}}
  {{{meta}}}
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0">
  <meta content="yes" name="apple-mobile-web-app-capable">
  <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
  <meta name="version" content="Afrostream ({{{version}}})">
  <meta name="msapplication-TileColor" content="#17071d">
  <meta name="application-name" content="{{{name}}}">

  <meta name="apple-itunes-app" content="{{{iosAppId}}}">
  <meta name="google-play-app" content="{{{androidAppId}}}">
  <link rel="apple-touch-icon" href="/images/icon175x175.jpeg">
  <link rel="android-touch-icon" href="/images/icon175x175.jpeg">
  {{{link}}}
  {{#each cssPaths}}
  <link rel="stylesheet" href="{{.}}">
  {{/each}}
  <!-- Facebook Pixel Code -->
  <script>
    !function (f, b, e, v, n, t, s) {
      if (f.fbq)return;
      n = f.fbq = function () {
        n.callMethod ?
          n.callMethod.apply(n, arguments) : n.queue.push(arguments)
      };
      if (!f._fbq)f._fbq = n;
      n.push = n;
      n.loaded = !0;
      n.version = '2.0';
      n.queue = [];
      t = b.createElement(e);
      t.async = !0;
      t.src = v;
      s = b.getElementsByTagName(e)[0];
      s.parentNode.insertBefore(t, s)
    }(window,
      document, 'script', 'https://connect.facebook.net/en_US/fbevents.js');

    fbq('init', '1594872720779391');
  </script>
  <noscript><img height="1" width="1" style="display:none"
                 src="https://www.facebook.com/tr?id=1594872720779391&ev=PageView&noscript=1"
  /></noscript>
  <!-- End Facebook Pixel Code -->
</head>
<body>
{{hash}}

<div id="main">{{{body}}}</div>

<script>
  __INITIAL_STATE__ = {{{json-stringify initialState}}};
</script>

{{#each jsPaths}}
<script src="{{.}}" type="text/javascript"></script>
{{/each}}

<!-- javascripts externals -->
{{#each externalsJs}}
<script async defer src="{{.}}" type="text/javascript"></script>
{{/each}}

<script src="https://www.gstatic.com/firebasejs/live/3.0/firebase.js"></script>
</body>
</html>
